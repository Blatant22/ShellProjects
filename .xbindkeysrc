# When I experimented with openbox, I used xbindkeys for keyboard shortcuts.
#----------------------------------------------------------------------------------

"xbindkeys_show" 
  control+shift + q

"bash $HOME/.i3a/use_dmenu"
  Mod1 + d

"bash $HOME/.i3a/terminal"
  Mod1 + Return

"bash $HOME/.i3a/scrot_sel_take"
  shift + Print

"bash $HOME/.i3a/scrot_take"
  Print

"bash $HOME/.i3a/track_notify"
  Mod4 + p

"/usr/bin/openbox --restart"
  shift+Mod1 + r

"/usr/bin/openbox --reconfigure"
  shift+Mod1 + c
