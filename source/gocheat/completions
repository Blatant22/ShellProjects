#!/usr/bin/env bash
#cito M:644 O:0 G:0 T:/usr/share/bash-completion/completions/gocheat
#------------------------------------------------------------------------------
# Project Name      - Extra/source/gocheat/completions
# Started On        - Mon  2 Mar 03:36:39 GMT 2020
# Last Change       - Thu  5 Mar 22:30:50 GMT 2020
